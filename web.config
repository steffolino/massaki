<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />	
		<staticContent>
		  <remove fileExtension=".woff" /> <!-- In case IIS already has this mime type -->
		  <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
		</staticContent>    		
    </system.webServer>
	
</configuration>
